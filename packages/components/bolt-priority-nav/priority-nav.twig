<bolt-priority-nav>
  <nav class="c-bolt-priority-nav">
    <bolt-nav-indicator>
      <ul class="c-bolt-priority-nav__list c-bolt-priority-nav__primary">
        {% for item in links %}
          <li class="c-bolt-priority-nav__item">
            {% include "@bolt-components-navlink/navlink.twig" with item only %}
          </li>
        {% endfor %}
      </ul>
    </bolt-nav-indicator>
  </nav>
</bolt-priority-nav>