    "url": "git+https://github.com/bolt-design-system/bolt.git"
  },
  "license": "MIT",
  "author": "Salem Ghoweri",
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/*/*",
      "packages/build-tools/plugins/*"
    ],
    "nohoist": [
      "lazysizes",
      "**/lazysizes",
      "get-own-property-symbols",
      "**/get-own-property-symbols"
    ]
  },
  "scripts": {
    "setup": "yarn",

    "start": "bolt start",
    "start:sass-import-test": "bolt start --config-file .boltrc--sass-import-test.js",
    
    "build": "bolt build --prod --no-ssr",
    "build:sass-import-test": "bolt build --prod --no-ssr"
  },
